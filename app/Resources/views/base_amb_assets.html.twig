<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Nemmi{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('/bundles/nemmiassets/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" />

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script type="application/javascript" src="{{ asset('/bundles/nemmiassets/bootstrap/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
